<launch>

<arg name="GT" value="trajectory_results/stamped_groundtruth.txt"/>
<arg name="Estimate" value="trajectory_results/stamped_traj_estimate.txt"/>

<node name="groundtruth" pkg="traj_result" type="traj_get_result" output="screen" args="$(arg GT)">
    <remap from="/trajectory" to="/gps/path"/>
</node>

<node name="estimate" pkg="traj_result" type="traj_get_result" output="screen" args="$(arg Estimate)">
    <remap from="/trajectory" to="/lvi_sam/lidar/mapping/path"/>
</node>

</launch>
